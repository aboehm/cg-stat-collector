{
	"mappings": {
		"_default_": {
			"_all": {
				"enabled": true,
				"norms": {
					"enabled": false
				}
			},
			"dynamic_templates": [
				{
					"template1": {
						"mapping": {
							"doc_values": true,
							"ignore_above": 1024,
							"index": "not_analyzed",
							"type": "{dynamic_type}"
						},
						"match": "*"
					}
				}
			],
			"properties": {
				"@timestamp": {
					"type": "date"
				},
				"seen": {
					"type": "date"
				},
				"name": {
					"type": "string"
				},
				"path": {
					"type": "string",
					"index": "not_analyzed"
				},
				"cgroup_type": {
					"type": "string"
				}
			}
		}
	},
	"settings": {
		"index.refresh_interval": "5s",
		"analysis": {
			"analyzer": {
				"default": {
					"tokenizer": "keyword"
				}
			}
		}
	},
	"template": "cgroup-*"
}

