{
	"mappings": {
		"_default_": {
			"_all": {
				"enabled": true,
				"norms": {
					"enabled": false
				}
			},
			"dynamic_templates": [
				{
					"template1": {
						"mapping": {
							"doc_values": true,
							"ignore_above": 1024,
							"index": "not_analyzed",
							"type": "{dynamic_type}"
						},
						"match": "*"
					}
				}
			],
			"properties": {
				"collected": {
					"properties": {
						"year": { "type": "integer" },
						"month": { "type": "integer" },
						"day": { "type": "integer" },
						"hour": { "type": "integer" },
						"minute": { "type": "integer" },
						"second": { "type": "integer" },
						"weekday": { "type": "string" },
						"utc": { "type": "date" },
						"localtime": { "type": "date" }
					}
				},
				"name": {
					"type": "string"
				},
				"path": {
					"type": "string",
					"index": "not_analyzed"
				}
			}
		}
	},
	"settings": {
		"index.refresh_interval": "5s",
		"analysis": {
			"analyzer": {
				"default": {
					"tokenizer": "keyword"
				}
			}
		}
	},
	"template": "collector-*"
}

